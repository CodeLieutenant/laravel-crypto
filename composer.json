{
  "name": "brossquad/laravel-crypto",
  "description": "Laravel Crypto is a package that provides a simple and easy to use API for encrypting, decrypting, hashing, and signing data using the latest PHP and Laravel features.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dusan Malusev",
      "email": "dusan@dusanmalusev.dev",
      "homepage": "https://www.dusanmalusev.dev",
      "role": "Developer"
    }
  ],
  "keywords": [
    "laravel",
    "security",
    "hashing",
    "signing",
    "encryption",
    "decryption",
    "crypto",
    "cryptography",
    "sodium"
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1",
    "ext-sodium": "*",
    "illuminate/support": "^8|^9|^10",
    "illuminate/contracts": "^8|^9|^10",
    "illuminate/config": "^8|^9|^10",
    "illuminate/hashing": "^8|^9|^10",
    "illuminate/encryption": "^8|^9|^10",
    "illuminate/collections": "^8|^9|^10"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpbench/phpbench": "@dev"
  },
  "autoload": {
    "psr-4": {
      "BrosSquad\\LaravelCrypto\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrosSquad\\LaravelCrypto\\Tests\\": "tests/",
      "BrosSquad\\LaravelCrypto\\Benchmarks\\": "benchmarks/"
    }
  },
  "suggest": {
    "ext-json": "Required for Json Serializer",
    "ext-msgpack": "Required for MessagePack Serializer",
    "ext-igbinary": "Required for Igbinary Serializer"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "BrosSquad\\LaravelCrypto\\ServiceProvider"
      ],
      "aliases": {
        "Hashing": "BrosSquad\\LaravelCrypto\\Facades\\Hashing",
        "Signing": "BrosSquad\\LaravelCrypto\\Facades\\Signing"
      }
    }
  }
}
