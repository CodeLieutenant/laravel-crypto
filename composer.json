{
  "name": "brossquad/laravel-crypto",
  "description": "Common Crypto algorithms used in php(laravel) applications",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dusan Malusev",
      "email": "dusan.998@outlook.com"
    }
  ],
  "minimum-stability": "stable",
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "require": {
    "php": ">=8.1",
    "ext-sodium": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "illuminate/support": "9.*|10.*",
    "illuminate/contracts": "9.*|10.*",
    "illuminate/config": "9.*|10.*",
    "illuminate/hashing": "9.*|10.*",
    "illuminate/encryption": "9.*|10.*",
    "illuminate/collections": "9.*|10.*"
  },
  "require-dev": {
    "orchestra/testbench": "^8",
    "phpbench/phpbench": "@dev"
  },
  "autoload": {
    "psr-4": {
      "BrosSquad\\LaravelCrypto\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrosSquad\\LaravelCrypto\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ServiceProvider"
      ],
      "aliases": {
        "Hashing": "BrosSquad\\LaravelCrypto\\Facades\\Hashing",
        "Signing": "BrosSquad\\LaravelCrypto\\Facades\\Signing"
      }
    }
  }
}
